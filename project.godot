; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Riddem"
run/main_scene="res://scenes/Menu.tscn"
config/use_custom_user_dir=true
boot_splash/image="res://banner.png"
config/icon="res://icon.png"
config/macos_native_icon="res://icon.icns"
config/windows_native_icon="res://icon.ico"

[audio]

output_latency=0
output_latency.web=0

[autoload]

Controller="*res://autoload/Controller.tscn"
Assets="*res://autoload/Assets.gd"
Riddem="*res://autoload/Riddem.gd"
Audio="*res://autoload/Audio.tscn"
Conductor="*res://autoload/Conductor.gd"
Save="*res://autoload/Save.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/vsync/vsync_via_compositor=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray(  )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_mouse_from_touch=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/fallback_to_gles2=true
quality/intended_usage/framebuffer_allocation=3
quality/intended_usage/framebuffer_allocation.mobile=false
vram_compression/import_etc=true
misc/lossless_compression/webp_compression_level=9
quality/shadows/filter_mode=0
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/shading/force_vertex_shading=true
quality/shading/force_lambert_over_burley=true
quality/shading/force_blinn_over_ggx=true
quality/depth_prepass/enable=false
gles3/shaders/shader_compilation_mode=2
gles3/shaders/shader_compilation_mode.mobile=2
gles3/shaders/shader_compilation_mode.web=2
quality/lightmapping/use_bicubic_sampling=false
environment/default_environment="res://default_env.tres"
